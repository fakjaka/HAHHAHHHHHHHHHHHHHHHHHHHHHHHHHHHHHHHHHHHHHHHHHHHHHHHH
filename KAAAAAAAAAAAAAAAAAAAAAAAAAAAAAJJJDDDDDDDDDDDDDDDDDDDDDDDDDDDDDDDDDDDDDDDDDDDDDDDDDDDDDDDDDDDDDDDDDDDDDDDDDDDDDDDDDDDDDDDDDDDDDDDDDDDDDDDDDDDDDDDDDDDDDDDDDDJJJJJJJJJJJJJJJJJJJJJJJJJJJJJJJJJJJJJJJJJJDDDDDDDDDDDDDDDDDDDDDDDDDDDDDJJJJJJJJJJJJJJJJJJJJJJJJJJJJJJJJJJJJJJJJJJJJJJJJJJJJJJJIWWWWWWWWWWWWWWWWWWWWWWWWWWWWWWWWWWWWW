-- Scripts:

local function FVZXDLW_fake_script() -- TextButtonDone.LocalScript 
	local script = Instance.new('LocalScript', TextButtonDone)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.TextBoxWarn.Text == "G7*L#Q1GS^MC<D}" then
			local StarterGui = game:GetService("StarterGui")
			StarterGui.ScreenGui.WarningFrame.Visible = false
		else
			script.Parent.Parent.TextBoxWarn.Text = "Incorrect"
		wait(5)
			script.Parent.MouseButton1Click:Connect(function()
				game.Players.LocalPlayer:Kick("Antivirus has saved you")
			end)
		end
	end)
end
coroutine.wrap(FVZXDLW_fake_script)()
local function MXLLHL_fake_script() -- TextButtonLINK.LocalScript 
	local script = Instance.new('LocalScript', TextButtonLINK)

	script.Parent.MouseButton1Click:Connect(function()
		setclipboard(cool)
	end)
	
end
coroutine.wrap(MXLLHL_fake_script)()
local function WALMZH_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local StarterGui = game:GetService("StarterGui")
	
	StarterGui.ScreenGui.ERRORS.Error1Frame.Visible = true
	wait(1)
	StarterGui.ScreenGui.ERRORS.Error2Frame.Visible = true
	wait(0.5)
	StarterGui.ScreenGui.ERRORS.Error3Frame.Visible = true
	wait(1)
	StarterGui.ScreenGui.ERRORS.Error4Frame.Visible = true
	wait(1)
	StarterGui.ScreenGui.ERRORS.Error5Frame.Visible = true
	wait(0.5)
	StarterGui.ScreenGui.ERRORS.Error6Frame.Visible = true
	wait(0.5)
	StarterGui.ScreenGui.ERRORS.Error7Frame.Visible = true
	wait(1)
	StarterGui.ScreenGui.ERRORS.Error8Frame.Visible = true
	wait(1)
	StarterGui.ScreenGui.ERRORS.Error9Frame.Visible = true
	wait(0.5)
	StarterGui.ScreenGui.ERRORS.Error10Frame.Visible = true
	wait(3)
	StarterGui.ScreenGui.WarningFrame.Visible = true
end
coroutine.wrap(WALMZH_fake_script)()
